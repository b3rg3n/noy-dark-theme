{
  "manifest_version": 3,
  "name": "NOY Dark Theme",
  "version": "1.0",
  "description": "Тёмная тема для NOY, чтоб глазки не текли\nMade by beergencheek epta",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://admin.psgateway.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}